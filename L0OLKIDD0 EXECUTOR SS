local BackBtn = createScriptButton("Back", 0.8)

local logMessages = {}

local function addLog(message)
    table.insert(logMessages, os.date("%H:%M:%S") .. ": " .. message)
    if #logMessages > 50 then
        table.remove(logMessages, 1)
    end
    print("L0OLKIDD0 SS Executor: " .. message)
end

local function safeExecute(code)
    local success, result = pcall(function()
        local fn, err = loadstring(code)
        if fn then
            return fn()
        end
        
        local fn2, err2 = loadstring("return function() " .. code .. " end")
        if fn2 then
            return fn2()()
        end
        
        local fn3, err3 = loadstring("(function() " .. code .. " end)()")
        if fn3 then
            return fn3()
        end
        
        error("All compilation methods failed: " .. tostring(err or err2 or err3))
    end)
    
    if success then
        addLog("Script executed successfully")
        return true
    else
        local errorMsg = tostring(result)
        addLog("Execution failed: " .. errorMsg)
        return false
    end
end

local function altExecute(code)
    local success1, result1 = pcall(function()
        local fn = loadstring(code)
        if fn then
            coroutine.wrap(fn)()
            return true
        end
        return false
    end)
    
    if success1 and result1 then
        addLog("Coroutine execution successful!")
        return true
    end
    
    local success2, result2 = pcall(function()
        local fn = loadstring(code)
        if fn then
            task.spawn(fn)
            return true
        end
        return false
    end)
    
    if success2 and result2 then
        addLog("Task execution successful!")
        return true
    end
    
    return false
end

local dragging = false
local dragStart
local startPos

local function updateInput(input)
    local delta = input.Position - dragStart
    MainFrame.Position = UDim2.new(
        startPos.X.Scale, 
        startPos.X.Offset + delta.X,
        startPos.Y.Scale, 
        startPos.Y.Offset + delta.Y
    )
end

TitleBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = MainFrame.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

TitleBar.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
        updateInput(input)
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
        updateInput(input)
    end
end)

OpenButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = true
    OpenButton.Visible = false
    addLog("L0OLKIDD0 SS Executor opened")
end)

CloseButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = false
    OpenButton.Visible = true
    addLog("L0OLKIDD0 SS Executor closed")
end)

ExecuteBtn.MouseButton1Click:Connect(function()
    local code = CodeInput.Text
    if code == "" then
        addLog("No code to execute")
        return
    end
    
    addLog("Executing script...")
    
    if not safeExecute(code) then
        addLog("Trying alternative methods...")
        if not altExecute(code) then
            addLog("All execution methods failed")
        end
    end
end)

ClearBtn.MouseButton1Click:Connect(function()
    CodeInput.Text = ""
    addLog("Code cleared")
end)

ScriptHubBtn.MouseButton1Click:Connect(function()
    ScriptHubFrame.Visible = true
    addLog("Script Hub opened")
end)

BackBtn.MouseButton1Click:Connect(function()
    ScriptHubFrame.Visible = false
    addLog("Returned to main screen")
end)

DexBtn.MouseButton1Click:Connect(function()
    local dexScript = 'loadstring(game:HttpGet("https://raw.githubusercontent.com/peyton2465/Dex/master/out.lua"))()'
    CodeInput.Text = dexScript
    ScriptHubFrame.Visible = false
    addLog("Dex Explorer loaded")
end)

TigerBtn.MouseButton1Click:Connect(function()
    local tigerScript = 'loadstring(game:HttpGet("https://raw.githubusercontent.com/balintTheDevX/Tiger-X-V3/main/Tiger%20X%20V3.5%20Fixed"))()'
    CodeInput.Text = tigerScript
    ScriptHubFrame.Visible = false
    addLog("Tiger X loaded")
end)

IYBtn.MouseButton1Click:Connect(function()
    local iyScript = 'loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()'
    CodeInput.Text = iyScript
    ScriptHubFrame.Visible = false
    addLog("Infinite Yield loaded")
end)

local glowConnection
local function startGlow()
    if glowConnection then
        glowConnection:Disconnect()
    end
    
    local time = 0
    glowConnection = RunService.Heartbeat:Connect(function(delta)
        time = time + delta
        local alpha = 0.5 + math.sin(time * 2) * 0.2
        mainStroke.Transparency = 1 - alpha
        TitleLabel.TextColor3 = Colors.Accent:Lerp(Color3.fromRGB(100, 200, 255), math.sin(time * 3) * 0.3)
    end)
end

OpenButton.MouseButton1Click:Connect(function()
    wait(0.1)
    startGlow()
end)

CloseButton.MouseButton1Click:Connect(function()
    if glowConnection then
        glowConnection:Disconnect()
    end
end)

addLog("L0OLKIDD0 SS Executor loaded!")
